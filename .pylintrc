[MASTER]
init-hook='import sys; sys.path.append("src")'

[MESSAGES CONTROL]
disable=
	broad-except,
	missing-class-docstring,
	missing-function-docstring,
	missing-module-docstring,
	no-else-return,
	no-self-use,
	too-few-public-methods,
	too-many-arguments,
	unspecified-encoding,

[FORMAT]
good-names=e

[IMPORTS]
known-third-party=
	mlflow,
